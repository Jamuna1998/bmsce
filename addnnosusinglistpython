def get():
    l=[]
    n=int(input("enter the size of list"))
    for i in range(n):
       y=int(input("enter the value"))
       l.append(y)   
    return l
        
          
def compute(length):
    mysum=0
    mysum=sum(length)
    return mysum

def output(mysum):
         print(mysum)

def main():
         n=0
         length=0
         length=get()
         mysum=compute(length)
         output(mysum)
         
main()  
    
