#include<stdio.h>
struct fractions
{
	int num;
	int deno;
};

struct fractions f[5];
int gcd(struct fractions);

void input(struct fractions *f,int *n)
{	int i;
	printf("enter the number of fractions\n");
	scanf("%d",n);
	printf("enter the fractions\n");
	for(i=0;i<*n;i++)
		{
		   printf("enter the numerator\n");
		   scanf("%d",&f->num);
		   printf("enter the denominator\n");
		   scanf("%d",&f->deno);
		}
}


void sum(struct fractions f,int n,int *sum)
{
	int i,*numerator,*denominator;
	*sum=0;
	for(i=0;i<n;i++)

	   {	*numerator=0;
		*numerator=(f.num*f.deno)+(f.deno*f.num);
		*denominator=f.deno*f.deno;
	   }
}


int main()
{
	struct fractions f;
	int n,c;
	input(&f,&n);
	sum(f,n,&c);
}
